<?xml version='1.0' encoding='utf-8'?>
<plugin id="cn.com.ths.device.manager" version="1.0.0"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <name>设备管理插件</name>
    <js-module name="ThsDeviceManager" src="www/thsdevicemanager.js">
        <clobbers target="cordova.plugins.thsdevicemanager" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="ThsDeviceManager">
                <param name="android-package" value="cn.com.ths.device.manager.ThsDeviceManager" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/ThsDeviceManager.java" target-dir="src/cn/com/ths/device/manager/ThsDeviceManager" />
        <source-file src="src/android/devicem.aar" target-dir="libs" />
        <source-file src="src/android/dpmapp.aar" target-dir="libs" />
        <source-file src="src/android/patternlocker.aar" target-dir="libs" />
        <framework custom="true" src="src/android/thsDeviceManager.gradle" type="gradleReference" />
        <config-file target="AndroidManifest.xml" parent="/manifest/application">
            <!--请填写你自己的 百度定位SDK api key -->
            <meta-data android:name="com.baidu.lbsapi.API_KEY" tools:replace="android:value" android:value="cU0kobAWaMKvarnVFYaDqMXcGcENWMoY"/>
            <meta-data android:name="com.huawei.hms.client.appid" android:value="appid=101968867" tools:replace="android:value"/>
             <!-- 请填写你自己的- appKey -->
            <meta-data android:name="com.alibaba.app.appkey" android:value="30134090" tools:replace="android:value"/>
            <!-- 请填写你自己的appSecret -->
            <meta-data android:name="com.alibaba.app.appsecret" android:value="d203f790978b3bf44edee87da123ddb2" tools:replace="android:value"/>
           
        </config-file>
    </platform>
</plugin>